{
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended",
      "tslint-sonarts"
    ],
    "jsRules": {},
    "rules": {
      "interface-name": [
        true,
        "never-prefix"
      ],
      "object-literal-sort-keys": false,
      "only-arrow-functions": false,
      "no-console": [
        true,
        "log"
      ]
    },
    "rulesDirectory": []
  }